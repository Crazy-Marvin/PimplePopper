<?xml version="1.0" encoding="utf-8"?>
<resources>
    <dimen name="abc_action_bar_content_inset_material">24dp</dimen>
    <dimen name="abc_action_bar_content_inset_with_nav">80dp</dimen>
    <dimen name="abc_action_bar_default_height_material">64dp</dimen>
    <dimen name="abc_action_bar_default_padding_end_material">8dp</dimen>
    <dimen name="abc_action_bar_default_padding_start_material">8dp</dimen>
    <dimen name="abc_config_prefDialogWidth">580dp</dimen>
    <dimen name="abc_text_size_subtitle_material_toolbar">16dp</dimen>
    <dimen name="abc_text_size_title_material_toolbar">20dp</dimen>
    <dimen name="ia_bg_corner_radius">3dp</dimen>
    <dimen name="ia_button_padding_lateral">48dp</dimen>
    <dimen name="ia_endcard_button_margin_bottom">74dp</dimen>
    <dimen name="ia_endcard_button_padding_vertical">8dp</dimen>
    <dimen name="ia_endcard_overlay_button_height_large">61dp</dimen>
    <dimen name="ia_endcard_overlay_button_height_medium">45dp</dimen>
    <dimen name="ia_endcard_overlay_button_width_large">247dp</dimen>
    <dimen name="ia_endcard_overlay_button_width_medium">185dp</dimen>
    <dimen name="ia_image_control_padding">5dp</dimen>
    <dimen name="ia_image_control_size">32dp</dimen>
    <dimen name="ia_inner_drawable_padding">4dp</dimen>
    <dimen name="ia_overlay_control_margin">12dp</dimen>
    <dimen name="ia_play_button_size">80dp</dimen>
    <dimen name="ia_progress_bar_height">2dp</dimen>
    <dimen name="ia_round_control_padding">8dp</dimen>
    <dimen name="ia_round_control_size">40dp</dimen>
    <dimen name="ia_round_control_size_clickable">64dp</dimen>
    <dimen name="ia_round_overlay_radius">18dp</dimen>
    <dimen name="ia_video_control_margin">8dp</dimen>
    <dimen name="ia_video_overlay_button_width">160dp</dimen>
    <dimen name="ia_video_overlay_text_large">23sp</dimen>
    <dimen name="ia_video_overlay_text_large_plus">27sp</dimen>
    <dimen name="ia_video_overlay_text_small">18sp</dimen>
    <dimen name="ia_video_text_padding">3dp</dimen>
    <integer name="ia_ib_button_size_dp">32</integer>
    <integer name="ia_ib_toolbar_height_dp">48</integer>
    <style name="ia_bottom_left_overlay">
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_marginBottom">@dimen/ia_video_control_margin</item>
        <item name="android:layout_marginLeft">@dimen/ia_video_control_margin</item>
    </style>
    <style name="ia_bottom_right_overlay">
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginBottom">@dimen/ia_video_control_margin</item>
        <item name="android:layout_marginRight">@dimen/ia_video_control_margin</item>
    </style>
    <style name="ia_expand_collapse_button_style" parent="ia_top_right_overlay">
        <item name="android:layout_width">@dimen/ia_image_control_size</item>
        <item name="android:layout_height">@dimen/ia_image_control_size</item>
        <item name="android:padding">@dimen/ia_image_control_padding</item>
        <item name="android:background">@drawable/ia_round_overlay_bg</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:src">@drawable/ia_sel_expand_collapse</item>
    </style>
    <style name="ia_mute_button_style" parent="ia_top_left_overlay">
        <item name="android:layout_width">@dimen/ia_image_control_size</item>
        <item name="android:layout_height">@dimen/ia_image_control_size</item>
        <item name="android:padding">@dimen/ia_image_control_padding</item>
        <item name="android:background">@drawable/bg_circle_overlay</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:src">@drawable/ia_sel_mute</item>
    </style>
    <style name="ia_play_button_style">
        <item name="android:layout_width">@dimen/ia_play_button_size</item>
        <item name="android:layout_height">@dimen/ia_play_button_size</item>
        <item name="android:src">@drawable/ia_play</item>
        <item name="android:layout_gravity">center</item>
    </style>
    <style name="ia_top_left_overlay">
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_marginTop">@dimen/ia_video_control_margin</item>
        <item name="android:layout_marginLeft">@dimen/ia_video_control_margin</item>
    </style>
    <style name="ia_top_right_overlay">
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginTop">@dimen/ia_video_control_margin</item>
        <item name="android:layout_marginRight">@dimen/ia_video_control_margin</item>
    </style>
    <style name="ia_tv_call_to_action_style" parent="ia_bottom_right_overlay">
        <item name="android:layout_width">@dimen/ia_video_overlay_button_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">@style/ia_video_overlay_text_view</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/ia_video_text_padding</item>
        <item name="android:background">@drawable/bg_text_overlay</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="ia_tv_remaining_time_style" parent="ia_bottom_left_overlay">
        <item name="android:layout_width">@dimen/ia_image_control_size</item>
        <item name="android:layout_height">@dimen/ia_image_control_size</item>
        <item name="android:textAppearance">@style/ia_video_overlay_text_view</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/bg_circle_overlay</item>
        <item name="android:maxLines">1</item>
    </style>
    <style name="ia_tv_skip_style" parent="ia_top_right_overlay">
        <item name="android:layout_width">@dimen/ia_video_overlay_button_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:padding">@dimen/ia_video_text_padding</item>
        <item name="android:background">@drawable/bg_text_overlay</item>
        <item name="android:textAppearance">@style/ia_video_overlay_text_view</item>
        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="ia_video_overlay_text_view">
        <item name="android:textSize">@dimen/ia_video_overlay_text_small</item>
        <item name="android:textColor">@color/ia_video_overlay_text</item>
        <item name="android:shadowColor">@color/ia_video_overlay_text_shadow</item>
        <item name="android:shadowRadius">0.5</item>
        <item name="android:shadowDx">1.5</item>
        <item name="android:shadowDy">1.0</item>
    </style>
    <style name="ia_video_progressbar_style" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/ia_progress_bar_height</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:progressDrawable">@drawable/ia_progress_bar_drawable</item>
    </style>
</resources>